<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student3" class="com.javalec.test6.Student">
		<constructor-arg value="김예슬"/>
		<constructor-arg value="23"/>
		<constructor-arg>
			<list>
				<value>술</value>
			</list>
		</constructor-arg>
		<property name="height" value="161"/>
		<property name="weight" value="46"/>
	</bean>
	<bean id="family" class="com.javalec.test6.Family" c:papaName="김재호" c:mamaName="장현숙" p:brotherName="김민규">
		<property name="sisterName" value="별,쁘니"/>
	</bean>
</beans>
